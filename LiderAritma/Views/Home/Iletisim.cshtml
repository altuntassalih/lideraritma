            <script type="text/javascript">
                $(document).ready(function () {
                    var dropDown = $('li.dropdown');
                    dropDown.on({
                        mouseenter: function () {
                            dropDown.clearQueue();
                            $(this).find('.dropdown-menu').fadeIn();
                        },
                        mouseleave: function () {
                            dropDown.find('.dropdown-menu').fadeOut();
                        }
                    });
                });
            </script>

